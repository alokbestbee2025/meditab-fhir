{
  "version": 2,
  "framework": "nuxtjs",
  "buildCommand": "npm run build",
  "outputDirectory": ".output",
  "builds": [
    {
      "src": ".output/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/.output/server/api/$1"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/.output/public/$1"
    }
  ]
}